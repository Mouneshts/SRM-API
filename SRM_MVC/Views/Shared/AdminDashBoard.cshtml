<!DOCTYPE html>
<html lang="en">
<head>
    <base href="./">
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <meta name="description" content="CoreUI - Open Source Bootstrap Admin Template">
    <meta name="author" content="Łukasz Holeczek">
    <meta name="keyword" content="Bootstrap,Admin,Template,Open,Source,jQuery,CSS,HTML,RWD,Dashboard">
    <title>SRM Admin</title>
    <style>
        .sidebar {
            background: #ffffff !important
        }

            .sidebar .nav-link {
                color: black !important;
            }

                .sidebar .nav-link:hover {
                    color: #fff !important;
                    background: #2d1967 !important;
                }

        .body {
            font-family: "Barlow", sans-serif !important;
        }
    </style>
    <!----Dashboard photos links---->
    <!-- Fontawseom Icon CSS -->
    <link rel="stylesheet" href="~/assets/css/all.css">

    <!-- Theme CSS -->
    <link rel="stylesheet" href="~/assets/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/assets/css/style.css">
    <link rel="stylesheet" href="~/assets/css/responsive.css">
    <!------------------------>
    <!-- Icons-->


    <link href="~/Layout/lib/flag-icon-css/css/flag-icon.min.css" rel="stylesheet">
    <link href="~/Layout/lib/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/Layout/lib/simple-line-icons/css/simple-line-icons.css" rel="stylesheet">
    <!-- Main styles for this application-->
    <link href="~/Layout/CoreUI/css/style.css" rel="stylesheet">
    <link href="~/Layout/CoreUI/vendors/pace-progress/css/pace.min.css" rel="stylesheet">

</head>
<body class="app header-fixed sidebar-fixed aside-menu-fixed sidebar-lg-show">
    <header class="app-header navbar">
        <button class="navbar-toggler sidebar-toggler d-lg-none mr-auto" type="button" data-toggle="sidebar-show">
            <span class="navbar-toggler-icon"></span>
        </button>
        <a class="navbar-brand" href="#">
            <img class="navbar-brand-full" src="~/Layout/img/LMS1.png" width="50" height="50" alt="CoreUI Logo">
            <img class="navbar-brand-minimized" src="~/Layout/img/LMS1.png" width="50" height="50" alt="CoreUI Logo">
        </a>
        <button class="navbar-toggler sidebar-toggler d-md-down-none" type="button" data-toggle="sidebar-lg-show">
            <span class="navbar-toggler-icon"></span>
        </button>

        <ul class="nav navbar-nav ml-auto">

            <li class="nav-item dropdown">
                <a class="nav-link" data-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">
                    <img class="img-avatar" src="~/Layout/img/33-338711_circle-user-icon-blue-hd-png-download.png" alt="admin@bootstrapmaster.com">
                </a>
                <div class="dropdown-menu dropdown-menu-right">

                    @* ADMIN Logout *@
                    <a class="nav-link" asp-action="LogOff" asp-controller="Admin">
                        <br>
                        <i class="nav-icon icon-logout"></i>Logout
                    </a>

                </div>
            </li>
        </ul>

    </header>
    <div class="app-body">
        <div class="sidebar">
            <nav class="sidebar-nav">
                <ul class="nav">
                    <li class="nav-item">
                        <h5 class="nav-link" style="pointer-events:none">
                            <i class="nav-icon icon-speedometer"></i> <strong>Dashboard</strong>
                            <!--<span class="badge badge-primary">NEW</span>-->
                        </h5>
                    </li>
                    <li class="nav-item nav-dropdown">
                        <a class="nav-link nav-dropdown-toggle" href="#">
                            <i class="nav-icon icon-people"></i> <strong>Admin</strong>
                        </a>

                        @*Admin DashBoard action links, below*@

                        <ul class="nav-dropdown-items">
                            <li class="nav-item">
                                <a class="nav-link" asp-action="AddAdmin" asp-controller="Admin">
                                    <i class="nav-icon icon-plus"></i> <strong>Add Admin</strong>
                                </a>
                            </li>

                            <li class="nav-item">
                                <a class="nav-link" asp-action="GetAdmins" asp-controller="Admin">
                                    <i class="nav-icon icon-people"></i> <strong>List All Admins</strong>
                                </a>
                            </li>
                        </ul>
                    </li>
                    <li class="nav-item nav-dropdown">
                        <a class="nav-link nav-dropdown-toggle" href="#">
                            <i class="nav-icon icon-book-open"></i> <strong>Courses</strong>
                        </a>
                        <ul class="nav-dropdown-items">
                            <li class="nav-item">
                                <a class="nav-link" asp-action="AddCourse" asp-controller="Course">
                                    <i class="nav-icon icon-plus"></i> <strong>Add Course</strong>
                                </a>

                            </li>


                            <li class="nav-item">
                                <a class="nav-link" asp-action="GetCourses" asp-controller="Course">
                                    <i class="nav-icon icon-check"></i> <strong>List All Courses</strong>
                                </a>
                            </li>
                        </ul>
                    </li>
                    <li class="nav-item nav-dropdown">
                        <a class="nav-link nav-dropdown-toggle" href="#">
                            <i class="nav-icon icon-book-open"></i> <strong>Branch</strong>
                        </a>
                        <ul class="nav-dropdown-items">
                            <li class="nav-item">
                                <a class="nav-link" asp-action="AddBranch" asp-controller="Branch">
                                    <i class="nav-icon icon-plus"></i> <strong>Add Branch</strong>
                                </a>

                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-action="GetBranchs" asp-controller="Branch">
                                    <i class="nav-icon icon-check"></i> <strong>List All Branch</strong>
                                </a>
                            </li>
                        </ul>
                    </li>
                    <li class="nav-item nav-dropdown">
                        <a class="nav-link nav-dropdown-toggle" href="#">
                            <i class="nav-icon icon-book-open"></i> <strong>Semester</strong>
                        </a>
                        <ul class="nav-dropdown-items">
                            <li class="nav-item">
                                <a class="nav-link" asp-action="AddSemester" asp-controller="Semester">
                                    <i class="nav-icon icon-plus"></i> <strong>Add Semester</strong>
                                </a>

                            <li class="nav-item">
                                <a class="nav-link" asp-action="GetSemesters" asp-controller="Semester">
                                    <i class="nav-icon icon-check"></i> <strong>List All Semesters</strong>
                                </a>
                            </li>
                        </ul>
                    </li>
                    <li class="nav-item nav-dropdown">
                        <a class="nav-link nav-dropdown-toggle" href="#">
                            <i class="nav-icon icon-book-open"></i> <strong>Subjects</strong>
                        </a>
                        <ul class="nav-dropdown-items">
                            <li class="nav-item">
                                <a class="nav-link" asp-action="AddSubject" asp-controller="Subject">
                                    <i class="nav-icon icon-plus"></i> <strong>Add Subject</strong>
                                </a>
                            <li class="nav-item">
                                <a class="nav-link" asp-action="GetSubjects" asp-controller="Subject">
                                    <i class="nav-icon icon-check"></i> <strong>List All Subjects</strong>
                                </a>
                            </li>
                        </ul>
                    </li>
                    <li class="nav-item nav-dropdown">
                        <a class="nav-link nav-dropdown-toggle" href="#">
                            <i class="nav-icon icon-people"></i> <strong>Student</strong>
                        </a>

                        @*Admin DashBoard action links, below*@

                        <ul class="nav-dropdown-items">
                            <li class="nav-item">
                                <a class="nav-link" asp-action="AddStudent" asp-controller="Student">
                                    <i class="nav-icon icon-plus"></i> <strong>Add Student</strong>
                                </a>
                            </li>

                            <li class="nav-item">
                                <a class="nav-link" asp-action="GetStudents" asp-controller="Student">
                                    <i class="nav-icon icon-people"></i> <strong>List All Students</strong>
                                </a>
                            </li>
                        </ul>
                    </li>
                    <li class="nav-item nav-dropdown">
                        <a class="nav-link nav-dropdown-toggle" href="#">
                            <i class="nav-icon icon-book-open"></i> <strong>Results</strong>
                        </a>
                        <ul class="nav-dropdown-items">
                            <li class="nav-item">
                                <a class="nav-link" asp-action="AddResult" asp-controller="Result">
                                    <i class="nav-icon icon-plus"></i> <strong>Add Result</strong>
                                </a>

                            </li>

                            <li class="nav-item">
                                <a class="nav-link" asp-action="GetResults" asp-controller="Result">
                                    <i class="nav-icon icon-check"></i> <strong>List All Results</strong>
                                </a>
                            </li>
                        </ul>
                    </li>
                    <li class="divider"></li>
                </ul>
            </nav>
            <button class="sidebar-minimizer brand-minimizer" type="button"></button>
        </div>
        <main class="main">

            <div class="container-fluid">
                @RenderBody()
            </div>

        </main>

    </div>
    <script src="~/Layout/lib/jquery/dist/jquery.min.js"></script>
    <script src="node_modules/popper.js/dist/umd/popper.min.js"></script>
    <script src="~/Layout/lib/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/Layout/lib/pace-progress/js/pace.min.js"></script>
    <script src="wwwwroot/Layout/lib/perfect-scrollbar/dist/perfect-scrollbar.min.js"></script>
    <script src="~/Layout/lib/coreui/dist/js/coreui.min.js"></script>
    <!-- Plugins and scripts required by this view-->
    <script src="node_modules/chart.js/dist/Chart.min.js"></script>

    <script src="js/main.js"></script>
</body>
</html>

